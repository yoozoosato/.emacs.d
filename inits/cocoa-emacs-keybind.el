;; keybindings for Mac
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

;; for Emacs >= 23 on Mac OS X
(setq ns-command-modifier (quote meta))
(setq ns-alternate-modifier (quote super))
(setq mac-option-modifier 'nil)

;; carbon emacs-like key bind
(global-set-key (kbd "M-<f1>") 'other-frame)
